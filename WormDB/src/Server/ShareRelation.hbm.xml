<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="server">
	<class name="ShareRelation" table="sharerelation">
		<composite-id name="RelationPK">
			<key-property name="recipientId" />
			<key-property name="fileId">
		</composite-id>		
	
		<property name="sharedDate">
			<column name="SharedDate" not-null="true"/>
		</property>
		
	</class>
</hibernate-mapping>