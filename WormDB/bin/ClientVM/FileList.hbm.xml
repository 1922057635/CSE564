<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="clientvm">
	<class name="FileList" table="filelist">
		<id name="fileId" column="FileID">
			<generator class="native" />
		</id>		
		<property name="fileName">
			<column name="FileName" length="20" not-null="true" />
		</property>
		
		<property name="fileSize">
			<column name="FileSize" not-null="true" />
		</property>
		
		<property name="shared">
			<column name="Shared" not-null="true"/>
		</property>
		
		<property name="uploadDate">
			<column name="UploadDate" not-null="true"/>
		</property>
		
	</class>
</hibernate-mapping>