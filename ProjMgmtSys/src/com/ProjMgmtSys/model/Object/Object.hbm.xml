<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ProjMgmtSys.model.Object">
	<class name="Object" table="object">
		<id name="objId" column="objId">
			<generator class="increment" />
		</id>	
		<property name="objName">
			<column name="objName" length="50" not-null="true" />
		</property>
		<property name="depId">
			<column name="depId" length="50" not-null="true" />
		</property>
		<property name="groId">
			<column name="groId" length="50" not-null="true" />
		</property>	
		<property name="rowNO">
			<column name="rowNO" length="50" not-null="true" />
		</property>			
	</class>
</hibernate-mapping>