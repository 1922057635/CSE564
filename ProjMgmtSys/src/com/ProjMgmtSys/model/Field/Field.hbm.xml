<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ProjMgmtSys.model.Field">
	<class name="Field" table="field">
		<id name="fieldId" column="fieldId">
			<generator class="increment" />
		</id>
		<property name="fieldName">
			<column name="fieldName" length="50" not-null="true" />
		</property>
		<property name="dataType">
			<column name="dataType" length="50" not-null="true" />
		</property>					
		<property name="objId">
			<column name="objId" length="50" not-null="true" />
		</property>
		<property name="depId">
			<column name="depId" length="50" not-null="true" />
		</property>
		<property name="groId">
			<column name="groId" length="50" not-null="true" />
		</property>				
	</class>
</hibernate-mapping>